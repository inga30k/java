public class Arreglos10 {

    public static void main(String[] args) {
      long[] Arreglo10=new long[10];
        for (int i = 0; i < 10; i++) {
          Arreglo10[i]=i*21;
                
                
            }
            System.out.println("Los valores de tipo long son:");
            for (int i = 0; i < 10; i++) {
                System.out.println(Arreglo10[i]+"");
              
        }
            
          
        }
        
    }
