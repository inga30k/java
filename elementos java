public class Elementos {    
    public static void main(String[] args) {
         int []contrase침a=new int[10];
        int[] elemenetos;
        Scanner Warriors = new Scanner(System.in);
        System.out.print("Ingrese el tama침o del arreglo (entre 10 y 20): ");
        int elementos = Warriors.nextInt();
        if (elementos < 10 || elementos > 20) {
            System.out.println("El tama침o del arreglo debe estar entre 10 y 20. Saliendo del programa.");
            
        }
        
        char[] arreglo = new char[elementos];
        for (int i = 0; i < elementos; ++i) {
            System.out.print("Ingrese un car치cter para el elemento " + (i + 1) + ": ");
            arreglo[i] = Warriors.next().charAt(10);
        }

        System.out.println("\nElementos del arreglo:");
        for (char elemento : arreglo) {
            System.out.print(elemento + " ");
        }

       
        
    }
}
